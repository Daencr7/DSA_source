#include <bits/stdc++.h>
#define el cout<<"\n"
using namespace std;

int main() {
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    int n;cin>>n;
    int ar[n];
    map <int, int>mp;
    for (int i = 0;i < n;i++){
        int x;cin>>x;
        if (mp[x]==0){
            cout << "1 ";
            mp[x]++;
        }
        else{
            cout<<mp[x] + 1 <<" ";
            mp[x]++;
        }
    }
    return 0;
}
